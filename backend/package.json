
{
  "name": "donation-campaign-e2e",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:backend": "npm run dev --prefix backend",
    "start:frontend": "npm start --prefix frontend",
    "start:all": "concurrently -k \"npm run start:backend\" \"npm run start:frontend\"",
    "test:run": "mocha -r ts-node/register tests/ui/**/*.test.ts --timeout 60000",
    "test:ui": "start-server-and-test \"npm run start:all\" http://localhost:3000 \"npm run test:run\""
  },
  "devDependencies": {
    "axios": "^1.5.0",
    "chai": "^4.4.1",
    "concurrently": "^8.2.2",
    "mocha": "^10.4.0",
    "selenium-webdriver": "^4.23.0",
    "start-server-and-test": "^2.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.1"
  }
}
